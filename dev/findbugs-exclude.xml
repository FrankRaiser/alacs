<?xml version="1.0" encoding="ISO-8859-1"?>
<!--
   A FindBugs filter for reducing false positives on bytecode
   generated by scalac.
-->
<FindBugsFilter>
  <!-- IRRELEVANT TO SCALA -->

  <!-- java keywords are irrelevant to scala -->
  <Match>
    <Bug pattern="NM_FUTURE_KEYWORD_USED_AS_MEMBER_IDENTIFIER" />
  </Match>

  <!-- SCALAC ARTIFACTS -->
  <Match>
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE" />
  </Match>

  <!-- POSTPONING CONSIDERATION -->
  <Match>
    <Bug code="NP" />
  </Match>
  <Match>
    <Bug code="Se" />
  </Match>
  <Match>
    <Bug category="BAD_PRACTICE" />
  </Match>
  <Match>
    <Priority value="2" />
  </Match>
  <Match>
    <Priority value="3" />
  </Match>
</FindBugsFilter>
